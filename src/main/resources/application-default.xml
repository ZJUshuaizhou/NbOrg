<!-- 
localRequestedAudiences和localRequestedRecipients可以配置多个url，默认为认证服务器地址+/oauth2/token

例如：authentication.properties配置的服务器地址为https://localhost:9443,
上述配置默认就为https://localhost:9443/oauth2/token
 -->
<Application>
	<ServiceProvider>
		<localApplicationIDTracker>true</localApplicationIDTracker>
		<localApplicationName></localApplicationName>
		<localApplicationNameTracker>true</localApplicationNameTracker>
		<localClaimConfig>
			<localAlwaysSendMappedLocalSubjectId>false
			</localAlwaysSendMappedLocalSubjectId>
			<localAlwaysSendMappedLocalSubjectIdTracker>true
			</localAlwaysSendMappedLocalSubjectIdTracker>
			<localClaimMappings>
				<ClaimMapping>
					<localDefaultValueTracker>false</localDefaultValueTracker>
					<localLocalClaim>
						<localClaimId>0</localClaimId>
						<localClaimIdTracker>true</localClaimIdTracker>
						<localClaimUri>http://wso2.org/claims/telephone</localClaimUri>
						<localClaimUriTracker>true</localClaimUriTracker>
					</localLocalClaim>
					<localLocalClaimTracker>true</localLocalClaimTracker>
					<localRemoteClaim>
						<localClaimId>0</localClaimId>
						<localClaimIdTracker>true</localClaimIdTracker>
						<localClaimUri>http://wso2.org/claims/telephone</localClaimUri>
						<localClaimUriTracker>true</localClaimUriTracker>
					</localRemoteClaim>
					<localRemoteClaimTracker>true</localRemoteClaimTracker>
					<localRequested>true</localRequested>
					<localRequestedTracker>true</localRequestedTracker>
				</ClaimMapping>
				<ClaimMapping>
					<localDefaultValueTracker>false</localDefaultValueTracker>
					<localLocalClaim>
						<localClaimId>0</localClaimId>
						<localClaimIdTracker>true</localClaimIdTracker>
						<localClaimUri>http://wso2.org/claims/role</localClaimUri>
						<localClaimUriTracker>true</localClaimUriTracker>
					</localLocalClaim>
					<localLocalClaimTracker>true</localLocalClaimTracker>
					<localRemoteClaim>
						<localClaimId>0</localClaimId>
						<localClaimIdTracker>true</localClaimIdTracker>
						<localClaimUri>http://wso2.org/claims/role</localClaimUri>
						<localClaimUriTracker>true</localClaimUriTracker>
					</localRemoteClaim>
					<localRemoteClaimTracker>true</localRemoteClaimTracker>
					<localRequested>true</localRequested>
					<localRequestedTracker>true</localRequestedTracker>
				</ClaimMapping>
				<ClaimMapping>
					<localDefaultValueTracker>false</localDefaultValueTracker>
					<localLocalClaim>
						<localClaimId>0</localClaimId>
						<localClaimIdTracker>true</localClaimIdTracker>
						<localClaimUri>http://wso2.org/claims/emailaddress</localClaimUri>
						<localClaimUriTracker>true</localClaimUriTracker>
					</localLocalClaim>
					<localLocalClaimTracker>true</localLocalClaimTracker>
					<localRemoteClaim>
						<localClaimId>0</localClaimId>
						<localClaimIdTracker>true</localClaimIdTracker>
						<localClaimUri>http://wso2.org/claims/emailaddress</localClaimUri>
						<localClaimUriTracker>true</localClaimUriTracker>
					</localRemoteClaim>
					<localRemoteClaimTracker>true</localRemoteClaimTracker>
					<localRequested>true</localRequested>
					<localRequestedTracker>true</localRequestedTracker>
				</ClaimMapping>
				<ClaimMapping>
					<localDefaultValueTracker>false</localDefaultValueTracker>
					<localLocalClaim>
						<localClaimId>0</localClaimId>
						<localClaimIdTracker>true</localClaimIdTracker>
						<localClaimUri>http://wso2.org/claims/organization</localClaimUri>
						<localClaimUriTracker>true</localClaimUriTracker>
					</localLocalClaim>
					<localLocalClaimTracker>true</localLocalClaimTracker>
					<localRemoteClaim>
						<localClaimId>0</localClaimId>
						<localClaimIdTracker>true</localClaimIdTracker>
						<localClaimUri>http://wso2.org/claims/organization</localClaimUri>
						<localClaimUriTracker>true</localClaimUriTracker>
					</localRemoteClaim>
					<localRemoteClaimTracker>true</localRemoteClaimTracker>
					<localRequested>true</localRequested>
					<localRequestedTracker>true</localRequestedTracker>
				</ClaimMapping>
			</localClaimMappings>
			<localClaimMappingsTracker>true</localClaimMappingsTracker>
			<localIdpClaimsTracker>false</localIdpClaimsTracker>
			<localLocalClaimDialect>true</localLocalClaimDialect>
			<localLocalClaimDialectTracker>true</localLocalClaimDialectTracker>
			<localRoleClaimURI></localRoleClaimURI>
			<localRoleClaimURITracker>true</localRoleClaimURITracker>
			<localUserClaimURITracker>false</localUserClaimURITracker>
		</localClaimConfig>
		<localClaimConfigTracker>true</localClaimConfigTracker>
		<localDescription></localDescription>
		<localDescriptionTracker>true</localDescriptionTracker>
		<localInboundAuthenticationConfig>
		</localInboundAuthenticationConfig>
		<localInboundAuthenticationConfigTracker>true
		</localInboundAuthenticationConfigTracker>
		<localInboundProvisioningConfig>
			<localProvisioningEnabled>false</localProvisioningEnabled>
			<localProvisioningEnabledTracker>true
			</localProvisioningEnabledTracker>
			<localProvisioningUserStore>PRIMARY</localProvisioningUserStore>
			<localProvisioningUserStoreTracker>true
			</localProvisioningUserStoreTracker>
		</localInboundProvisioningConfig>
		<localInboundProvisioningConfigTracker>true
		</localInboundProvisioningConfigTracker>
		<localLocalAndOutBoundAuthenticationConfig>
			<localAlwaysSendBackAuthenticatedListOfIdPs>false
			</localAlwaysSendBackAuthenticatedListOfIdPs>
			<localAlwaysSendBackAuthenticatedListOfIdPsTracker>true
			</localAlwaysSendBackAuthenticatedListOfIdPsTracker>
			<localAuthenticationStepForAttributesTracker>true
			</localAuthenticationStepForAttributesTracker>
			<localAuthenticationStepForSubjectTracker>true
			</localAuthenticationStepForSubjectTracker>
			<localAuthenticationStepsTracker>false
			</localAuthenticationStepsTracker>
			<localAuthenticationType>default</localAuthenticationType>
			<localAuthenticationTypeTracker>true</localAuthenticationTypeTracker>
			<localSubjectClaimUriTracker>false</localSubjectClaimUriTracker>
		</localLocalAndOutBoundAuthenticationConfig>
		<localLocalAndOutBoundAuthenticationConfigTracker>true
		</localLocalAndOutBoundAuthenticationConfigTracker>
		<localOutboundProvisioningConfig>
			<localProvisionByRoleList>
				<null />
			</localProvisionByRoleList>
			<localProvisionByRoleListTracker>true
			</localProvisionByRoleListTracker>
			<localProvisioningIdentityProvidersTracker>false
			</localProvisioningIdentityProvidersTracker>
		</localOutboundProvisioningConfig>
		<localOutboundProvisioningConfigTracker>true
		</localOutboundProvisioningConfigTracker>
		<localOwner>
			<localTenantId>-1234</localTenantId>
			<localTenantIdTracker>true</localTenantIdTracker>
			<localUserName>admin</localUserName>
			<localUserNameTracker>true</localUserNameTracker>
			<localUserStoreDomain>PRIMARY</localUserStoreDomain>
			<localUserStoreDomainTracker>true</localUserStoreDomainTracker>
		</localOwner>
		<localOwnerTracker>true</localOwnerTracker>
		<localPermissionAndRoleConfig>
			<localIdpRolesTracker>false</localIdpRolesTracker>
			<localPermissionsTracker>false</localPermissionsTracker>
			<localRoleMappingsTracker>false</localRoleMappingsTracker>
		</localPermissionAndRoleConfig>
		<localPermissionAndRoleConfigTracker>true
		</localPermissionAndRoleConfigTracker>
		<localRequestPathAuthenticatorConfigsTracker>false
		</localRequestPathAuthenticatorConfigsTracker>
		<localSaasApp>true</localSaasApp>
		<localSaasAppTracker>true</localSaasAppTracker>
	</ServiceProvider>


	<OAuth>
		<localOAuthVersion>OAuth-2.0</localOAuthVersion>
		<localOAuthVersionTracker>true</localOAuthVersionTracker>
		<localApplicationName></localApplicationName>
		<localApplicationNameTracker>true</localApplicationNameTracker>
		<localCallbackUrl></localCallbackUrl>
		<localCallbackUrlTracker>true</localCallbackUrlTracker>
		<localGrantTypes>authorization_code implicit password
			client_credentials refresh_token
			urn:ietf:params:oauth:grant-type:saml2-bearer iwa:ntlm
		</localGrantTypes>
		<localGrantTypesTracker>true</localGrantTypesTracker>
		<localOauthConsumerKey></localOauthConsumerKey>
		<localOauthConsumerKeyTracker>true</localOauthConsumerKeyTracker>
		<localOauthConsumerSecret></localOauthConsumerSecret>
		<localOauthConsumerSecretTracker>true
		</localOauthConsumerSecretTracker>
		<localUsernameTracker>false</localUsernameTracker>
	</OAuth>



	<SAML>
		<localAssertionConsumerUrl>
		</localAssertionConsumerUrl>
		<localAssertionConsumerUrlTracker>true
		</localAssertionConsumerUrlTracker>
		<localAttributeConsumingServiceIndex>
		</localAttributeConsumingServiceIndex>
		<localAttributeConsumingServiceIndexTracker>true
		</localAttributeConsumingServiceIndexTracker>
		<localCertAliasTracker>false</localCertAliasTracker>
		<localDoEnableEncryptedAssertion>false
		</localDoEnableEncryptedAssertion>
		<localDoEnableEncryptedAssertionTracker>true
		</localDoEnableEncryptedAssertionTracker>
		<localDoSignAssertions>true</localDoSignAssertions>
		<localDoSignAssertionsTracker>true</localDoSignAssertionsTracker>
		<localDoSignResponse>true</localDoSignResponse>
		<localDoSignResponseTracker>true</localDoSignResponseTracker>
		<localDoSingleLogout>true</localDoSingleLogout>
		<localDoSingleLogoutTracker>true</localDoSingleLogoutTracker>
		<localDoValidateSignatureInRequests>false
		</localDoValidateSignatureInRequests>
		<localDoValidateSignatureInRequestsTracker>true
		</localDoValidateSignatureInRequestsTracker>
		<localEnableAttributesByDefault>true</localEnableAttributesByDefault>
		<localEnableAttributesByDefaultTracker>true
		</localEnableAttributesByDefaultTracker>
		<localIdPInitSSOEnabled>false</localIdPInitSSOEnabled>
		<localIdPInitSSOEnabledTracker>true</localIdPInitSSOEnabledTracker>
		<localIssuer></localIssuer>
		<localIssuerTracker>true</localIssuerTracker>
		<localLoginPageURL></localLoginPageURL>
		<localLoginPageURLTracker>true</localLoginPageURLTracker>
		<localLogoutURL>
		</localLogoutURL>
		<localLogoutURLTracker>true</localLogoutURLTracker>
		<localNameIDFormat>urn/oasis/names/tc/SAML/1.1/nameid-format/emailAddress
		</localNameIDFormat>
		<localNameIDFormatTracker>true</localNameIDFormatTracker>
		<localNameIdClaimUriTracker>false</localNameIdClaimUriTracker>
		<localRequestedAudiences>
			<string></string>
		</localRequestedAudiences>
		<localRequestedAudiencesTracker>true</localRequestedAudiencesTracker>
		<localRequestedClaims>
			<null />
		</localRequestedClaims>
		<localRequestedClaimsTracker>true</localRequestedClaimsTracker>
		<localRequestedRecipients>
			<string></string>
		</localRequestedRecipients>
		<localRequestedRecipientsTracker>true
		</localRequestedRecipientsTracker>
		<localUseFullyQualifiedUsername>true</localUseFullyQualifiedUsername>
		<localUseFullyQualifiedUsernameTracker>true
		</localUseFullyQualifiedUsernameTracker>
	</SAML>
	<!-- 
		这里配置的是STS协议默认使用的证书别名，确保认证服务器上的证书库中包含此证书别名
	 -->
	<STS>
		<DefaultCertAlias>wso2carbon.cert</DefaultCertAlias>
	</STS>
</Application>